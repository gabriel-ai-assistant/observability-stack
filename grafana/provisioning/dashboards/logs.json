{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Log Volume Over Time",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 0 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (container) (count_over_time({job=~\".+\"} [1m]))",
          "legendFormat": "{{container}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": { "drawStyle": "bars", "stacking": { "mode": "normal" } },
          "unit": "short"
        },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "right" } }
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum(count_over_time({job=~\".+\"} |~ \"(?i)(level=error|err|ERROR)\" [1m]))",
          "legendFormat": "Errors/min",
          "refId": "A"
        },
        {
          "expr": "sum(count_over_time({job=~\".+\"} [1m]))",
          "legendFormat": "Total/min",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": { "custom": { "drawStyle": "line", "fillOpacity": 10 }, "unit": "short" },
        "overrides": [
          { "matcher": { "id": "byName", "options": "Errors/min" }, "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }] }
        ]
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "title": "Top Error Messages",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "topk(20, sum by (message) (count_over_time({job=~\".+\"} |~ \"(?i)(error|err|ERROR)\" | pattern \"<message>\" [5m])))",
          "legendFormat": "",
          "refId": "A",
          "instant": true
        }
      ],
      "fieldConfig": { "defaults": {}, "overrides": [] },
      "options": { "showHeader": true, "sortBy": [{ "desc": true, "displayName": "Value" }] },
      "transformations": [
        { "id": "sortBy", "options": { "fields": {}, "sort": [{ "field": "Value", "desc": true }] } }
      ]
    },
    {
      "title": "Logs by Service/Container",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 16 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "sum by (compose_service) (count_over_time({job=~\".+\"} [1m]))",
          "legendFormat": "{{compose_service}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "custom": { "drawStyle": "bars", "stacking": { "mode": "normal" } }, "unit": "short" },
        "overrides": []
      },
      "options": { "tooltip": { "mode": "multi" }, "legend": { "displayMode": "table", "placement": "right" } }
    },
    {
      "title": "Log Search",
      "type": "logs",
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 24 },
      "datasource": { "type": "loki", "uid": "loki" },
      "targets": [
        {
          "expr": "{job=~\".+\"} |~ \"$search\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": true,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "sortOrder": "Descending",
        "dedupStrategy": "none"
      }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "tags": ["logs", "loki"],
  "templating": {
    "list": [
      {
        "name": "search",
        "type": "textbox",
        "label": "Search",
        "current": { "value": "" },
        "options": []
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Logs Overview",
  "uid": "logs-overview",
  "version": 1
}
